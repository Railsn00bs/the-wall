{
  "name":"the-wall",
  "scripts": {
    "postdeploy": "bundle exec rake db:migrate && bundle exec rake db:seed"
  },
  "env":{
    "LANG":{
      "required":true
    },
    "RACK_ENV": "development",
    "RAILS_ENV": "development",
    "RAILS_SERVE_STATIC_FILES":{
      "required":true
    },
    "SECRET_KEY_BASE":{
      "required":true
    }
  },
  "addons":[
    "heroku-postgresql"
  ]
}
